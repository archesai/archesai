{
  "name": "@archesai/platform",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#app/*": "./src/app/*.tsx",
    "#components/*": "./src/components/*.tsx",
    "#lib/*": "./src/lib/*.ts",
    "#router": "./src/router.tsx"
  },
  "scripts": {
    "build": "vite build",
    "clean": "git clean -xdf node_modules .cache .turbo .tanstack dist .nitro .out",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 vite dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "vite start",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier/base",
  "dependencies": {
    "@archesai/client": "workspace:*",
    "@archesai/schemas": "workspace:*",
    "@archesai/ui": "workspace:*",
    "@tanstack/react-query": "catalog:",
    "@tanstack/react-query-devtools": "catalog:",
    "@tanstack/react-router": "catalog:",
    "@tanstack/react-router-devtools": "catalog:",
    "@tanstack/react-router-with-query": "catalog:",
    "@tanstack/react-start": "catalog:",
    "@xyflow/react": "catalog:",
    "motion": "^12.23.6",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "vite": "catalog:"
  },
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
    "@fontsource/geist": "catalog:",
    "@fontsource/geist-mono": "catalog:",
    "@tailwindcss/vite": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "@vitejs/plugin-react": "catalog:",
    "babel-plugin-react-compiler": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:"
  }
}
