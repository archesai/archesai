{
  "name": "@archesai/platform",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#app/*": "./src/app/*.tsx",
    "#components/*": "./src/components/*.tsx",
    "#hooks/*": "./src/hooks/*.tsx",
    "#lib/*": "./src/lib/*.ts"
  },
  "scripts": {
    "build": "next build",
    "clean": "rm -rf .cache .turbo .next dist node_modules",
    "dev": "next dev --turbo --hostname 0.0.0.0",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "next start --hostname 0.0.0.0",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@archesai/client": "workspace:*",
    "@archesai/domain": "workspace:*",
    "@archesai/ui": "workspace:*",
    "@hookform/resolvers": "^5.0.1",
    "@sinclair/typebox": "^0.34.33",
    "@tanstack/react-query": "^5.76.1",
    "@xyflow/react": "^12.6.4",
    "date-fns": "^4.1.0",
    "geist": "^1.4.2",
    "lucide-react": "^0.511.0",
    "next": "^15.3.2",
    "next-themes": "^0.4.6",
    "nuqs": "^2.4.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "sonner": "^2.0.3"
  }
}
