{
  "name": "@archesai/archesai",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "clean": "pnpm clean:dist && pnpm clean:node_modules",
    "clean:dist": "pnpm -r exec sh -c 'rm -rf .cache .turbo .tanstack dist .nitro .output'",
    "clean:node_modules": "pnpm -r exec sh -c 'rm -rf node_modules'",
    "client:generate": "pnpm --filter @archesai/client client:generate",
    "dev:platform": "pnpm -F @archesai/platform dev",
    "format": "prettier --ignore-path .gitignore --ignore-path .prettierignore '**/*.{js,ts,tsx,json,css,md,yaml,gitignore,prettierignore}' --list-different",
    "format:fix": "prettier --ignore-path .gitignore --ignore-path .prettierignore '**/*.{js,ts,tsx,json,css,md,yaml,gitignore,prettierignore}' --write --list-different",
    "preinstall": "only-allow pnpm",
    "postinstall": "sherif",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint --fix",
    "prepare": "husky",
    "test": "pnpm -r test",
    "test:coverage": "pnpm -r test --coverage",
    "test:ui": "pnpm -r test --ui",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier",
  "devDependencies": {
    "@archesai/prettier": "workspace:*",
    "@commitlint/cli": "catalog:root",
    "@commitlint/config-conventional": "catalog:root",
    "@redocly/cli": "catalog:",
    "eslint": "catalog:eslint",
    "husky": "catalog:root",
    "only-allow": "catalog:root",
    "prettier": "catalog:prettier",
    "sherif": "catalog:root",
    "tailwindcss": "catalog:ui",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=24.7.0",
    "pnpm": "^10.15.1"
  }
}
