{
  "name": "@archesai/archesai",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "pnpm clean:dist && pnpm clean:node_modules",
    "clean:dist": "rm -rf dist .next .cache tsconfig.tsbuildinfo tsconfig.lib.tsbuildinfo tsconfig.spec.tsbuildinfo && pnpm -r exec rm -rf dist .next .nx .cache tsconfig.tsbuildinfo tsconfig.lib.tsbuildinfo tsconfig.spec.tsbuildinfo",
    "clean:node_modules": "rm -rf pnpm-lock.yaml node_modules && pnpm -r exec rm -rf pnpm-lock.yaml node_modules && pnpm store prune",
    "preinstall": "pnpm only-allow pnpm",
    "postinstall": "pnpm sherif",
    "prepare": "husky",
    "validate": "nx run-many --all -t typecheck build lint --verbose --skip-nx-cache"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,md,yaml}": [
      "pnpm prettier --write --ignore-path .gitignore --log-level warn"
    ]
  },
  "prettier": "@archesai/prettier/base",
  "devDependencies": {
    "@archesai/eslint": "workspace:*",
    "@archesai/jest": "workspace:*",
    "@archesai/next": "workspace:*",
    "@archesai/prettier": "workspace:*",
    "@archesai/swc": "workspace:*",
    "@archesai/tailwind": "workspace:*",
    "@archesai/typescript": "workspace:*",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "@golevelup/ts-jest": "^0.7.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@ibm-cloud/openapi-ruleset": "^1.31.1",
    "@nestjs/testing": "^11.1.1",
    "@next/eslint-plugin-next": "^15.3.2",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.18",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/passport-twitter": "^1.0.40",
    "@types/pg": "^8.15.2",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/supertest": "^6.0.3",
    "drizzle-kit": "^0.31.1",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-openapi": "^0.14.2",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.0.0",
    "next": "^15.3.2",
    "only-allow": "^1.2.1",
    "orval": "^7.9.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.13",
    "prettier-plugin-sort-json": "^4.1.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sherif": "^1.5.0",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.1.7",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=23.9.0",
    "pnpm": "^10.11.0"
  }
}
