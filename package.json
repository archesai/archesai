{
  "name": "@archesai/archesai",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "clean": "pnpm -r exec sh -c 'rm -rf .cache .tanstack dist .nitro .output'",
    "clean:deps": "pnpm -r exec sh -c 'rm -rf node_modules pnpm-lock.yaml'",
    "generate": "pnpm -r generate",
    "dev": "pnpm -r dev",
    "format": "pnpm prettier . --write --check --log-level warn --cache --cache-location .cache/.prettiercache",
    "postinstall": "sh -c 'if [ -z \"$CI\" ]; then pnpm dlx sherif --fix; else pnpm dlx sherif; fi'",
    "lint": "pnpm -r lint --cache --cache-location .cache/.eslintcache",
    "test": "pnpm -r test",
    "typecheck": "pnpm tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier",
  "devDependencies": {
    "@archesai/prettier": "workspace:*",
    "task-master-ai": "catalog:"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=24.7.0",
    "pnpm": "^10.15.1"
  }
}
