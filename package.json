{
  "name": "@archesai/archesai",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "clean": "pnpm -r exec sh -c 'rm -rf .cache .tanstack dist .nitro .output node_modules'",
    "generate": "pnpm -r generate",
    "dev": "pnpm -r dev",
    "format": "pnpm prettier --ignore-path .gitignore --ignore-path .prettierignore '**/*.{css,html,js,json,jsonc,md,ts,tsx,yaml}' --check --write --log-level warn",
    "preinstall": "pnpm dlx only-allow pnpm",
    "postinstall": "pnpm dlx sherif --fix",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "typecheck": "pnpm tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier",
  "devDependencies": {
    "@archesai/prettier": "workspace:*",
    "eslint": "catalog:eslint",
    "prettier": "catalog:prettier",
    "tailwindcss": "catalog:ui",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=24.7.0",
    "pnpm": "^10.15.1"
  }
}
