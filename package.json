{
  "name": "@archesai/archesai",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nx run-many -t build -t tsc:build",
    "clean": "pnpm clean:dist && pnpm clean:node_modules",
    "clean:dist": "pnpm -r exec sh -c 'rm -rf .cache .turbo .tanstack dist .nitro .output .nx'",
    "clean:node_modules": "pnpm -r exec sh -c 'rm -rf node_modules'",
    "client:generate": "pnpm --filter @archesai/client client:generate",
    "dev:platform": "nx dev platform",
    "format": "prettier --ignore-path .gitignore --ignore-path .prettierignore '**/*.{js,ts,tsx,json,css,md,yaml,gitignore,prettierignore}' --list-different",
    "format:fix": "prettier --ignore-path .gitignore --ignore-path .prettierignore '**/*.{js,ts,tsx,json,css,md,yaml,gitignore,prettierignore}' --write",
    "preinstall": "only-allow pnpm",
    "postinstall": "sherif && prettier --write pnpm-lock.yaml",
    "lint": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "prepare": "husky",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "nx run-many -t typecheck"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,css,md,gitignore,prettierignore}": [
      "pnpm format"
    ]
  },
  "prettier": "@archesai/prettier",
  "devDependencies": {
    "@archesai/eslint": "workspace:*",
    "@archesai/prettier": "workspace:*",
    "@archesai/typescript": "workspace:*",
    "@commitlint/cli": "catalog:root",
    "@commitlint/config-conventional": "catalog:root",
    "@fontsource/geist": "catalog:ui",
    "@fontsource/geist-mono": "catalog:ui",
    "@nx/eslint": "catalog:nx",
    "@nx/eslint-plugin": "catalog:nx",
    "@nx/js": "catalog:nx",
    "@playwright/test": "catalog:test",
    "@redocly/cli": "catalog:",
    "@tailwindcss/vite": "catalog:vite",
    "@tanstack/react-router": "catalog:tanstack",
    "@tanstack/table-core": "catalog:tanstack",
    "@types/node": "catalog:",
    "@types/qs": "catalog:",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "@vitejs/plugin-react": "catalog:vite",
    "@vitest/ui": "catalog:test",
    "babel-plugin-react-compiler": "catalog:vite",
    "core-js": "catalog:",
    "eslint": "catalog:eslint",
    "husky": "catalog:root",
    "lint-staged": "catalog:root",
    "nx": "catalog:nx",
    "only-allow": "catalog:root",
    "openapi-types": "^12.1.3",
    "orval": "catalog:client",
    "prettier": "catalog:prettier",
    "rollup-plugin-visualizer": "catalog:vite",
    "sherif": "catalog:root",
    "tailwindcss": "catalog:ui",
    "tw-animate-css": "catalog:ui",
    "typescript": "catalog:",
    "vite": "catalog:vite",
    "vitest": "catalog:test",
    "zod": "catalog:"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=24.7.0",
    "pnpm": "^10.15.1"
  }
}
