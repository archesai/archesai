{
  "name": "@archesai/ui",
  "version": "0.0.0",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "imports": {
    "#globals.css": "./src/styles/globals.css",
    "#atoms/*": "./src/atoms/*.ts",
    "#components/*": "./src/components/*.tsx",
    "#hooks/*": "./src/hooks/*.tsx",
    "#layouts/*": "./src/layouts/*.tsx",
    "#lib/*": "./src/lib/*.ts",
    "#providers/*": "./src/providers/*.tsx",
    "#types/*": "./src/types/*.ts"
  },
  "exports": {
    "./globals.css": "./src/styles/globals.css",
    "./atoms/*": "./src/atoms/*.ts",
    "./components/*": "./src/components/*.tsx",
    "./hooks/*": "./src/hooks/*.tsx",
    "./layouts/*": "./src/layouts/*.tsx",
    "./lib/*": "./src/lib/*.ts",
    "./providers/*": "./src/providers/*.tsx",
    "./types/*": "./src/types/*.ts",
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint .",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "dependencies": {
    "@dnd-kit/core": "catalog:ui",
    "@dnd-kit/modifiers": "catalog:ui",
    "@dnd-kit/sortable": "catalog:ui",
    "@dnd-kit/utilities": "catalog:ui",
    "@radix-ui/react-slot": "catalog:ui",
    "@tanstack/react-query": "catalog:tanstack",
    "@tanstack/react-router": "catalog:tanstack",
    "@tanstack/react-table": "catalog:tanstack",
    "class-variance-authority": "catalog:ui",
    "clsx": "catalog:ui",
    "cmdk": "catalog:ui",
    "date-fns": "catalog:ui",
    "lucide-react": "catalog:ui",
    "motion": "catalog:ui",
    "next-themes": "catalog:ui",
    "radix-ui": "catalog:ui",
    "react": "catalog:react19",
    "react-day-picker": "catalog:ui",
    "react-dom": "catalog:react19",
    "react-hook-form": "catalog:ui",
    "react-resizable-panels": "catalog:ui",
    "socket.io-client": "catalog:",
    "sonner": "catalog:ui",
    "tailwind-merge": "catalog:ui",
    "vaul": "catalog:ui"
  },
  "devDependencies": {
    "@archesai/eslint": "workspace:*",
    "@fontsource/geist": "catalog:ui",
    "@fontsource/geist-mono": "catalog:ui",
    "@tanstack/table-core": "catalog:tanstack",
    "@types/node": "catalog:",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "@vitest/ui": "catalog:test",
    "eslint": "catalog:eslint",
    "tw-animate-css": "catalog:ui",
    "typescript": "catalog:",
    "vitest": "catalog:test"
  }
}
