{
  "name": "@archesai/jest",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf node_modules .cache .turbo .tanstack dist .nitro .out",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier/base",
  "dependencies": {
    "@archesai/swc": "workspace:*",
    "@testing-library/dom": "catalog:",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/user-event": "catalog:",
    "jest-environment-jsdom": "catalog:"
  },
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "jest": "catalog:",
    "typescript": "catalog:"
  }
}
