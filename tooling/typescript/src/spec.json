{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "tsBuildInfoFile": "${configDir}/.cache/tsconfig.spec.tsbuildinfo",
    "types": ["node", "vitest/globals"]
  },
  "exclude": ["${configDir}/node_modules", "${configDir}/dist"],
  "include": ["${configDir}/src/**/*.spec.ts", "${configDir}/src/**/*.spec.tsx"]
}
