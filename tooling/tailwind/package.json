{
  "name": "@archesai/tailwind",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./src/*.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    "./native": "./src/native.ts",
    "./web": "./src/web.ts",
    "./postcss": "./src/postcss.ts"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf node_modules .cache .turbo .next dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@archesai/prettier/base",
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
    "@types/node": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:"
  }
}
