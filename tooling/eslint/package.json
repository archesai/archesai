{
  "name": "@archesai/eslint",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "build": "tsc --build",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier/base",
  "dependencies": {
    "@eslint/compat": "catalog:eslint",
    "@eslint/js": "catalog:eslint",
    "eslint-config-prettier": "catalog:eslint",
    "eslint-import-resolver-typescript": "catalog:eslint",
    "eslint-plugin-import": "catalog:eslint",
    "eslint-plugin-jest": "catalog:eslint",
    "eslint-plugin-jsdoc": "catalog:eslint",
    "eslint-plugin-perfectionist": "catalog:eslint",
    "eslint-plugin-react": "catalog:eslint",
    "eslint-plugin-react-hooks": "catalog:eslint",
    "globals": "catalog:eslint",
    "typescript-eslint": "catalog:eslint"
  },
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
    "@types/node": "catalog:",
    "typescript": "catalog:"
  }
}
