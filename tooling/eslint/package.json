{
  "name": "@archesai/eslint",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf node_modules .cache .turbo .tanstack dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier/base",
  "dependencies": {
    "@eslint/compat": "catalog:",
    "@eslint/js": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-import-resolver-typescript": "catalog:",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-jest": "catalog:",
    "eslint-plugin-jsdoc": "catalog:",
    "eslint-plugin-perfectionist": "catalog:",
    "eslint-plugin-react": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "globals": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
    "@types/node": "catalog:",
    "typescript": "catalog:"
  }
}
