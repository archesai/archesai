{
  "name": "@archesai/prettier",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./base": "./src/base.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo .next dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  }
}
