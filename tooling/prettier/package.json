{
  "name": "@archesai/prettier",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./base": "./src/base.js"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf node_modules .cache .turbo .next dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@archesai/prettier/base",
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "prettier-plugin-packagejson": "^2.5.14",
    "prettier-plugin-sort-json": "^4.1.1",
    "prettier-plugin-tailwindcss": "^0.6.11"
  },
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
    "@types/node": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  }
}
