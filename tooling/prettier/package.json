{
  "name": "@archesai/prettier",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": "./src/*.js"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf node_modules .cache .turbo .next dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier/base",
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "catalog:",
    "prettier-plugin-packagejson": "catalog:",
    "prettier-plugin-sh": "catalog:",
    "prettier-plugin-sort-json": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:"
  },
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
    "@types/node": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  }
}
