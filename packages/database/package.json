{
  "name": "@archesai/database",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./src/*.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "scripts": {
    "database:clean": "node ./src/utils/clean.ts",
    "database:generate": "drizzle-kit generate",
    "database:inspect": "drizzle-kit inspect",
    "database:migrate": "drizzle-kit migrate",
    "database:push": "drizzle-kit push",
    "database:reset": "node ./src/utils/reset.ts",
    "database:seed": "node ./src/utils/seed.ts",
    "database:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@archesai/core": "workspace:*",
    "@archesai/schemas": "workspace:*",
    "drizzle-orm": "catalog:database",
    "drizzle-seed": "catalog:database",
    "pg": "catalog:database"
  }
}
