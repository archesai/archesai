{
  "name": "@archesai/ui",
  "version": "0.0.0",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "imports": {
    "#globals.css": "./src/styles/globals.css",
    "#atoms/*": "./src/atoms/*.ts",
    "#components/*": "./src/components/*.tsx",
    "#hooks/*": "./src/hooks/*.tsx",
    "#layouts/*": "./src/layouts/*.tsx",
    "#lib/*": "./src/lib/*.ts",
    "#providers/*": "./src/providers/*.tsx"
  },
  "exports": {
    "./package.json": "./package.json",
    "./globals.css": "./src/styles/globals.css",
    "./atoms/*": "./src/atoms/*.ts",
    "./components/*": "./src/components/*.tsx",
    "./hooks/*": "./src/hooks/*.tsx",
    "./layouts/*": "./src/layouts/*.tsx",
    "./lib/*": "./src/lib/*.ts",
    "./providers/*": "./src/providers/*.tsx"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf node_modules .cache .turbo .tanstack dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier/base",
  "dependencies": {
    "@archesai/client": "workspace:*",
    "@archesai/core": "workspace:*",
    "@archesai/domain": "workspace:*",
    "@hookform/resolvers": "catalog:",
    "@sinclair/typebox": "catalog:",
    "@tanstack/react-query": "catalog:",
    "@tanstack/react-table": "catalog:",
    "class-variance-authority": "catalog:",
    "clsx": "catalog:",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "jotai": "^2.12.5",
    "lucide-react": "catalog:",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.2",
    "react": "catalog:react19",
    "react-day-picker": "^9.8.0",
    "react-dom": "catalog:react19",
    "react-hook-form": "catalog:",
    "react-player": "^3.1.0",
    "react-resizable-panels": "^3.0.3",
    "socket.io-client": "catalog:",
    "sonner": "^2.0.6",
    "tailwind-merge": "catalog:",
    "tslib": "catalog:",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@archesai/jest": "workspace:*",
    "@archesai/typescript": "workspace:*",
    "@tanstack/react-router": "catalog:",
    "@tanstack/table-core": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "jest": "catalog:",
    "tailwindcss": "catalog:",
    "tw-animate-css": "catalog:",
    "typescript": "catalog:"
  }
}
