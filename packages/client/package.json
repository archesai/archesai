{
  "name": "@archesai/client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./src/*.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf node_modules .cache .turbo .tanstack dist .nitro .out",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "generate": "NODE_TLS_REJECT_UNAUTHORIZED=0 orval",
    "lint": "eslint",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier/base",
  "dependencies": {
    "@tanstack/react-query": "catalog:",
    "qs": "catalog:",
    "tslib": "catalog:"
  },
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
    "@ibm-cloud/openapi-ruleset": "catalog:",
    "@types/node": "catalog:",
    "@types/qs": "catalog:",
    "orval": "catalog:",
    "typescript": "catalog:"
  }
}
