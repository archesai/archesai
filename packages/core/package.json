{
  "name": "@archesai/core",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./src/*.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf node_modules .cache .turbo .tanstack dist .nitro .out",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier/base",
  "dependencies": {
    "@archesai/schemas": "workspace:*",
    "@fastify/deepmerge": "catalog:fastify",
    "@redis/client": "catalog:",
    "@sinclair/typebox": "catalog:",
    "@socket.io/redis-adapter": "catalog:",
    "nodemailer": "catalog:",
    "pino": "catalog:",
    "socket.io": "catalog:",
    "tslib": "catalog:",
    "yaml": "catalog:"
  },
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
    "@fastify/type-provider-typebox": "catalog:fastify",
    "@types/node": "catalog:",
    "@types/nodemailer": "catalog:",
    "fastify": "catalog:fastify",
    "pino-pretty": "catalog:",
    "typescript": "catalog:"
  }
}
