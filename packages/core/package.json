{
  "name": "@archesai/core",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./dist/*.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf node_modules .cache .turbo .next dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier/base",
  "dependencies": {
    "@archesai/domain": "workspace:*",
    "@fastify/cors": "catalog:",
    "@fastify/deepmerge": "catalog:",
    "@fastify/swagger": "catalog:",
    "@fastify/type-provider-typebox": "catalog:",
    "@scalar/fastify-api-reference": "catalog:",
    "@sinclair/typebox": "catalog:",
    "@socket.io/redis-adapter": "catalog:",
    "fastify": "catalog:",
    "nodemailer": "catalog:",
    "pino": "catalog:",
    "redis": "catalog:",
    "reflect-metadata": "catalog:",
    "socket.io": "catalog:",
    "tslib": "catalog:",
    "yaml": "catalog:"
  },
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
    "@golevelup/ts-jest": "catalog:",
    "@nestjs/testing": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "@types/nodemailer": "catalog:",
    "jest": "catalog:",
    "pino-pretty": "catalog:",
    "typescript": "catalog:"
  }
}
