    "@scalar/fastify-api-reference": "^1.31.2",
    "@sinclair/typebox": "catalog:",
    "@socket.io/redis-adapter": "^8.3.0",
    "fastify": "^5.3.3",
    "nodemailer": "^7.0.3",
    "pino": "^9.7.0",
    "redis": "^5.1.1",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "tslib": "catalog:",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
