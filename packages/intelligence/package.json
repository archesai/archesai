{
  "name": "@archesai/intelligence",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./dist/*.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf node_modules .cache .turbo .next dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier/base",
  "dependencies": {
    "@archesai/core": "workspace:*",
    "@elevenlabs/elevenlabs-js": "catalog:",
    "@sinclair/typebox": "catalog:",
    "fluent-ffmpeg": "catalog:",
    "openai": "catalog:",
    "playwright-core": "catalog:",
    "sharp": "catalog:",
    "tslib": "catalog:"
  },
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
    "@golevelup/ts-jest": "catalog:",
    "@nestjs/testing": "catalog:",
    "@types/fluent-ffmpeg": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "jest": "catalog:",
    "typescript": "catalog:"
  }
}
