{
  "name": "@archesai/intelligence",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./src/*.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf node_modules .cache .turbo .tanstack dist .nitro .out",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier/base",
  "dependencies": {
    "@archesai/core": "workspace:*",
    "@archesai/schemas": "workspace:*",
    "@elevenlabs/elevenlabs-js": "catalog:",
    "fluent-ffmpeg": "catalog:",
    "openai": "catalog:",
    "playwright-core": "catalog:",
    "sharp": "catalog:",
    "tslib": "catalog:"
  },
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
    "@golevelup/ts-jest": "catalog:",
    "@nestjs/testing": "catalog:",
    "@types/fluent-ffmpeg": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "jest": "catalog:",
    "typescript": "catalog:"
  }
}
