{
  "name": "@archesai/billing",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./dist/*.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf .cache .turbo .next dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@archesai/core": "workspace:*",
    "@archesai/domain": "workspace:*",
    "@sinclair/typebox": "^0.34.33",
    "stripe": "^18.1.0",
    "tslib": "^2.8.1"
  }
}
