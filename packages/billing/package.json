{
  "name": "@archesai/billing",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./src/*.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf node_modules .cache .turbo .next dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --build --emitDeclarationOnly"
  },
  "prettier": "@archesai/prettier/base",
  "dependencies": {
    "@archesai/core": "workspace:*",
    "@archesai/domain": "workspace:*",
    "@sinclair/typebox": "catalog:",
    "stripe": "catalog:",
    "tslib": "catalog:"
  },
  "devDependencies": {
    "@archesai/typescript": "workspace:*",
    "@types/node": "catalog:",
    "typescript": "catalog:"
  }
}
