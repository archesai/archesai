  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run db:reset && TS_JEST=true jest --config ./test/jest.config.ts",
    "migrations:dev": "npx prisma migrate dev",
    "migrations:prod": "npx prisma migrate deploy",
    "db:seed": "npx ts-node prisma/seed.ts",
    "db:reset": "npx prisma migrate reset --force && npx prisma migrate dev",
    "seed": "npm run db:reset && npm run db:seed"
  },
