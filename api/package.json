{
  "name": "api",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix .",
    "tsc": "tsc --noEmit --pretty",
    "test": "jest --color",
    "test:watch": "jest --watch --color",
    "test:cov": "jest --coverage --color",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "TS_JEST=true jest --config ./test/jest.config.ts --color $FILE_NAME",
    "migrations:dev": "prisma migrate dev",
    "migrations:prod": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset --force",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node --transpile-only -r tsconfig-paths/register prisma/seed.ts"
  },
