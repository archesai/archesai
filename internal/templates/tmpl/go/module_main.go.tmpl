{{template "header" .}}
package main

import (
	"log"

	"{{ .ProjectName }}/app"
)

func main() {
	a := app.NewApp()
	if err := a.Initialize(); err != nil {
		log.Fatalf("failed to initialize app: %v", err)
	}
	if err := a.Start(); err != nil {
		log.Fatalf("app error: %v", err)
	}
}
