{{template "header" .}}
package main

import (
	"log"

	"{{ .ProjectName }}/bootstrap"
)

func main() {
	app := bootstrap.NewApp()
	if err := app.Initialize(); err != nil {
		log.Fatalf("failed to initialize app: %v", err)
	}
	if err := app.Start(); err != nil {
		log.Fatalf("app error: %v", err)
	}
}
